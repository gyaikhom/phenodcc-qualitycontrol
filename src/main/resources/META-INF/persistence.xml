        <class>org.mousephenotype.dcc.entities.overviews.Strain</class>
        <class>org.mousephenotype.dcc.entities.overviews.AnimalOverview</class>
        <class>org.mousephenotype.dcc.entities.overviews.MeasurementsPerformed</class>
        <class>org.mousephenotype.dcc.entities.overviews.ProceduresPerformed</class>
        <class>org.mousephenotype.dcc.entities.overviews.ProcedureAnimalOverview</class>
        <class>org.mousephenotype.dcc.entities.overviews.MetadataGroupToValues</class>
        <class>org.mousephenotype.dcc.entities.overviews.ProcedureMetadataGroup</class>

        <!-- we need entities in the Quality Control entities packages -->
        <class>org.mousephenotype.dcc.entities.qc.IssueStatus</class>
        <class>org.mousephenotype.dcc.entities.qc.DataContext</class>
        <class>org.mousephenotype.dcc.entities.qc.AnIssue</class>
        <class>org.mousephenotype.dcc.entities.qc.AnAction</class>
        <class>org.mousephenotype.dcc.entities.qc.ActionType</class>
        <class>org.mousephenotype.dcc.entities.qc.CitedDataPoint</class>
        <class>org.mousephenotype.dcc.entities.qc.History</class>
        <class>org.mousephenotype.dcc.entities.qc.AState</class>

