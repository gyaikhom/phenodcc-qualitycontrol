        <class>org.mousephenotype.dcc.entities.media.FileExtension</class>
        <class>org.mousephenotype.dcc.entities.media.MediaPhase</class>
        <class>org.mousephenotype.dcc.entities.media.MediaFile</class>
        <class>org.mousephenotype.dcc.entities.media.ErrorLogs</class>
        <class>org.mousephenotype.dcc.entities.media.MediaStatus</class>
        <class>org.mousephenotype.dcc.entities.media.Association</class>

